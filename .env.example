# Copy this file to .env and fill in the required secrets before running the container.

# Hugging Face access token for downloading private models
HUGGINGFACE_TOKEN=

# Docker registry credentials (used in CI/CD pipelines)
DOCKER_USERNAME=
DOCKER_PASSWORD=

# RunPod API token for deployment automation
RUNPOD_API_KEY=

# Optional runtime configuration
GRADIO_SHARE=false
GRADIO_PORT=7860
MODEL_CACHE_DIR=/workspace/weights
OUTPUT_DIR=/workspace/output
